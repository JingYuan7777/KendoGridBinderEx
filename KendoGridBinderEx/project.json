{
    "version": "5.0.2.0",
    "title": "KendoGridBinderEx",
    "description": "This is an extension on the KendoGridBinder which uses AutoMapper to support mapping from ViewModel <> Entity.",
    "authors": [ "Stef Heyenrath" ],

    "packOptions": {
        "summary": "This is an extension on the KendoGridBinder which uses AutoMapper to support mapping from ViewModel <> Entity.",
        "tags": [ "Kendo", "Grid", "Model", "AutoMapper", "MVC", "ModelBinder", "Binder" ],
        "owners": [ "Stef Heyenrath" ],
        "repository": {
            "type": "git",
            "url": "https://github.com/StefH/KendoGridBinderEx"
        },
        "projectUrl": "https://github.com/StefH/KendoGridBinderEx",
        "licenseUrl": "https://raw.githubusercontent.com/StefH/KendoGridBinderEx/master/LICENSE",
        "releaseNotes": "Use AutoMapper version 5.1.1 and added support for netstandard1.6"
    },

    "buildOptions": {
    },

    "dependencies": {
        "JetBrains.Annotations": {
            "version": "10.1.4",
            "type": "build"
        },
        "AutoMapper": "5.1.1",
        "System.Linq.Dynamic.Core": "1.0.6.8"
    },

    "frameworks": {
        "net45": {
            "frameworkAssemblies": {
                "System.Web": "",
                "System.Runtime.Serialization": ""
            },
            "dependencies": {
                "Newtonsoft.Json": "6.0.8",

                "Microsoft.AspNet.Mvc": "5.2.3",
                "Microsoft.AspNet.WebApi.Client": "5.2.3",
                "Microsoft.AspNet.WebApi.Core": "5.2.3"
            }
        },
        "netstandard1.6": {
            "buildOptions": { "define": [ "NETSTANDARD" ] },
            "imports": [
                "dotnet5.4"
            ],
            "dependencies": {
                "Newtonsoft.Json": "9.0.1",

                "System.Collections.Specialized": "4.3.0",
                "Microsoft.AspNetCore.Mvc": "1.0.1",
                "Microsoft.AspNetCore.Mvc.Core": "1.0.1"
            }
        }
    }
}